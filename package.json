{
  "name": "budgetkey-ng2-components-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build budgetkey-ng2-components --watch",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "fetch-theme": "echo 'export const DEFAULT_THEME='`curl https://raw.githubusercontent.com/OpenBudget/budgetkey-themes/master/theme.budgetkey.he.json | json_pp -json_op canonical`';' > projects/budgetkey-ng2-components/src/lib/theme.budgetkey.he.ts",
    "copy-license": "cp LICENSE dist/budgetkey-ng2-components",
    "copy-readme": "cp README.md dist/budgetkey-ng2-components",
    "copy-styles": "cp -r projects/budgetkey-ng2-components/src/styles/* dist/budgetkey-ng2-components",
    "copy-assets": "cp -r projects/budgetkey-ng2-components/src/assets dist/budgetkey-ng2-components",
    "copy-files": "npm run copy-license && npm run copy-readme && npm run copy-assets && npm run copy-styles",
    "build_lib": "ng build budgetkey-ng2-components",
    "npm_pack": "cd dist/budgetkey-ng2-components && npm pack",
    "package": "npm run fetch-theme && npm run build_lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run package && npm publish ./dist/budgetkey-ng2-components/budgetkey-ng2-components*.tgz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "budgetkey-ng2-auth": "^1.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "bootstrap": "^3.4.1",
    "bootstrap-rtl": "^3.3.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "node-sass": "^6.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
